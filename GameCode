        Dim p7g12 As String = txtplayer7guess12.Text
        Dim p7g13 As String = txtplayer7guess13.Text
        Dim p7g14 As String = txtplayer7guess14.Text
        'player 1 guess 1 score
       
        If txtplayer1guess1.Text = "" Or txtplayer1trick1.Text = "" Then
            lblp1s1.Text = ""
        ElseIf txtplayer1guess1.Text = 0 And txtplayer1trick1.Text = 0 Then

            lblp1s1.Text = 10

        ElseIf txtplayer1guess1.Text = p1g1 And txtplayer1trick1.Text = p1g1 Then

            lblp1s1.Text = p1g1 * 11
        ElseIf txtplayer1guess1.Text <> txtplayer1trick1.Text Then

            Dim y As Integer
            y = Math.Abs(p1g1 - txtplayer1trick1.Text)
            lblp1s1.Text = y * -10

        End If

        'player 1 guess 2 score


        If txtplayer1guess2.Text = "" Or txtplayer1trick2.Text = "" Then
            lblp1s2.Text = ""
        ElseIf txtplayer1guess2.Text = 0 And txtplayer1trick2.Text = 0 Then

            lblp1s2.Text = 10

        ElseIf txtplayer1guess2.Text = p1g2 And txtplayer1trick2.Text = p1g2 Then

            lblp1s2.Text = p1g2 * 11
        ElseIf txtplayer1guess2.Text <> txtplayer1trick2.Text Then

            Dim y As Integer
            y = Math.Abs(p1g2 - txtplayer1trick2.Text)
            lblp1s2.Text = y * -10

        End If


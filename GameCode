            lblp7s8.Text = y * -10
        End If

        'Player 7 guess 9 score


        If txtplayer7guess9.Text = "" Or txtplayer7trick9.Text = "" Then
            lblp7s9.Text = ""
        ElseIf txtplayer7guess9.Text = 0 And txtplayer7trick9.Text = 0 Then

            lblp7s9.Text = 10

        ElseIf txtplayer7guess9.Text = txtplayer7trick9.Text Then

            lblp7s9.Text = p7g9 * 11

        ElseIf txtplayer7guess9.Text <> txtplayer7trick9.Text Then
            Dim y As Integer
            y = Math.Abs(p7g9 - txtplayer7trick9.Text)
            lblp7s9.Text = y * -10

        End If


        'Player 7 guess 10
        If txtplayer7guess10.Text = "" Or txtplayer7trick10.Text = "" Then
            lblp7s10.Text = ""
        ElseIf txtplayer7guess10.Text = 0 And txtplayer7trick10.Text = 0 Then

            lblp7s10.Text = 10

        ElseIf txtplayer7guess10.Text = p7g10 And txtplayer7trick10.Text = p7g10 Then

            lblp7s10.Text = p7g10 * 11
        ElseIf txtplayer7guess10.Text <> txtplayer7trick10.Text Then

            Dim x As Integer
            x = Math.Abs(p7g10 - txtplayer7trick10.Text)
            lblp7s10.Text = x * -10

        End If
        'Player 7 guess 11 score


        If txtplayer7guess11.Text = "" Or txtplayer7trick11.Text = "" Then
            lblp7s11.Text = ""
        ElseIf txtplayer7guess11.Text = 0 And txtplayer7trick11.Text = 0 Then

            lblp7s11.Text = 10

        ElseIf txtplayer7guess11.Text = p7g11 And txtplayer7trick11.Text = p7g11 Then

            lblp7s11.Text = p7g11 * 11
        ElseIf txtplayer7guess11.Text <> txtplayer7trick11.Text Then

            Dim x As Integer
            x = Math.Abs(p7g11 - txtplayer7trick11.Text)
            lblp7s11.Text = x * -10

        End If

        'Player 7 guess 12 score

        If txtplayer7guess12.Text = "" Or txtplayer7trick12.Text = "" Then
            lblp7s12.Text = ""
        ElseIf txtplayer7guess12.Text = 0 And txtplayer7trick12.Text = 0 Then

            lblp7s12.Text = 10

        ElseIf txtplayer7guess12.Text = p7g12 And txtplayer7trick12.Text = p7g12 Then

            lblp7s12.Text = p7g12 * 11
        ElseIf txtplayer7guess12.Text <> txtplayer7trick12.Text Then
            Dim x As Integer
            x = Math.Abs(p7g12 - txtplayer7trick12.Text)
            lblp7s12.Text = x * -10


        End If
        'Player 7 guess 13 score

        If txtplayer7guess13.Text = "" Or txtplayer7trick13.Text = "" Then
            lblp7s13.Text = ""
        ElseIf txtplayer7guess13.Text = 0 And txtplayer7trick13.Text = 0 Then

            lblp7s13.Text = 10

        ElseIf txtplayer7guess13.Text = txtplayer7trick13.Text Then

            lblp7s13.Text = p7g13 * 11

        ElseIf txtplayer7guess13.Text <> txtplayer7trick13.Text Then
            Dim x As Integer
            x = Math.Abs(p7g13 - txtplayer7trick13.Text)
            lblp7s13.Text = x * -10


        End If
        'Player 7 guess 14 score
        If txtplayer7guess14.Text = "" Or txtplayer7trick14.Text = "" Then
            lblp7s14.Text = ""
        ElseIf txtplayer7guess14.Text = 0 And txtplayer7trick14.Text = 0 Then

            lblp7s14.Text = 10

        ElseIf txtplayer7guess14.Text = p7g14 And txtplayer7trick14.Text = p7g14 Then

            lblp7s14.Text = p7g14 * 11
        ElseIf txtplayer7guess14.Text <> txtplayer7trick14.Text Then

            Dim x As Integer
            x = Math.Abs(p7g14 - txtplayer7trick14.Text)
            lblp7s14.Text = x * -10

        End If

        txtp1score.Text = Val(lblp1s1.Text) + Val(lblp1s2.Text) + Val(lblp1s3.Text) + Val(lblp1s4.Text) + Val(lblp1s5.Text) + Val(lblp1s6.Text) + Val(lblp1s7.Text) + Val(lblp1s8.Text) + Val(lblp1s9.Text) + Val(lblp1s10.Text) + Val(lblp1s11.Text) + Val(lblp1s12.Text) + Val(lblp1s13.Text) + Val(lblp1s14.Text)
        txtp2score.Text = Val(lblp2s1.Text) + Val(lblp2s2.Text) + Val(lblp2s3.Text) + Val(lblp2s4.Text) + Val(lblp2s5.Text) + Val(lblp2s6.Text) + Val(lblp2s7.Text) + Val(lblp2s8.Text) + Val(lblp2s9.Text) + Val(lblp2s10.Text) + Val(lblp2s11.Text) + Val(lblp2s12.Text) + Val(lblp2s13.Text) + Val(lblp2s14.Text)
        txtp3score.Text = Val(lblp3s1.Text) + Val(lblp3s2.Text) + Val(lblp3s3.Text) + Val(lblp3s4.Text) + Val(lblp3s5.Text) + Val(lblp3s6.Text) + Val(lblp3s7.Text) + Val(lblp3s8.Text) + Val(lblp3s9.Text) + Val(lblp3s10.Text) + Val(lblp3s11.Text) + Val(lblp3s12.Text) + Val(lblp3s13.Text) + Val(lblp3s14.Text)
        txtp4score.Text = Val(lblp4s1.Text) + Val(lblp4s2.Text) + Val(lblp4s3.Text) + Val(lblp4s4.Text) + Val(lblp4s5.Text) + Val(lblp4s6.Text) + Val(lblp4s7.Text) + Val(lblp4s8.Text) + Val(lblp4s9.Text) + Val(lblp4s10.Text) + Val(lblp4s11.Text) + Val(lblp4s12.Text) + Val(lblp4s13.Text) + Val(lblp4s14.Text)
        txtp5score.Text = Val(lblp5s1.Text) + Val(lblp5s2.Text) + Val(lblp5s3.Text) + Val(lblp5s4.Text) + Val(lblp5s5.Text) + Val(lblp5s6.Text) + Val(lblp5s7.Text) + Val(lblp5s8.Text) + Val(lblp5s9.Text) + Val(lblp5s10.Text) + Val(lblp5s11.Text) + Val(lblp5s12.Text) + Val(lblp5s13.Text) + Val(lblp5s14.Text)
        txtp6score.Text = Val(lblp6s1.Text) + Val(lblp6s2.Text) + Val(lblp6s3.Text) + Val(lblp6s4.Text) + Val(lblp6s5.Text) + Val(lblp6s6.Text) + Val(lblp6s7.Text) + Val(lblp6s8.Text) + Val(lblp6s9.Text) + Val(lblp6s10.Text) + Val(lblp6s11.Text) + Val(lblp6s12.Text) + Val(lblp6s13.Text) + Val(lblp6s14.Text)
        txtp7score.Text = Val(lblp7s1.Text) + Val(lblp7s2.Text) + Val(lblp7s3.Text) + Val(lblp7s4.Text) + Val(lblp7s5.Text) + Val(lblp7s6.Text) + Val(lblp7s7.Text) + Val(lblp7s8.Text) + Val(lblp7s9.Text) + Val(lblp7s10.Text) + Val(lblp7s11.Text) + Val(lblp7s12.Text) + Val(lblp7s13.Text) + Val(lblp7s14.Text)

        If txtp1score.Text < 0 Then
            txtp1score.ForeColor = Color.Red

        ElseIf txtp1score.Text >= 0 Then
            txtp1score.ForeColor = Color.Black
        End If

        If txtp2score.Text < 0 Then
            txtp2score.ForeColor = Color.Red

        ElseIf txtp2score.Text >= 0 Then
            txtp2score.ForeColor = Color.Black
        End If
        If txtp3score.Text < 0 Then
            txtp3score.ForeColor = Color.Red

        ElseIf txtp3score.Text >= 0 Then
            txtp3score.ForeColor = Color.Black
        End If

        If txtp4score.Text < 0 Then
            txtp4score.ForeColor = Color.Red

        ElseIf txtp4score.Text >= 0 Then
            txtp4score.ForeColor = Color.Black
        End If

        If txtp5score.Text < 0 Then
            txtp5score.ForeColor = Color.Red

        ElseIf txtp5score.Text >= 0 Then
            txtp5score.ForeColor = Color.Black
        End If
        If txtp6score.Text < 0 Then
            txtp6score.ForeColor = Color.Red

        ElseIf txtp6score.Text >= 0 Then
            txtp6score.ForeColor = Color.Black
        End If

        If txtp7score.Text < 0 Then
            txtp7score.ForeColor = Color.Red

        ElseIf txtp7score.Text >= 0 Then
            txtp7score.ForeColor = Color.Black
        End If

        If txtplayer1guess14.Text <> "" Then
            btnnewgame.Visible = True

        End If
        
    End Sub


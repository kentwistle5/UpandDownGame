    Private Sub txtplayer7trick12_KeyPress(ByVal sender As System.Object, ByVal e As System.Windows.Forms.KeyPressEventArgs) Handles txtplayer7trick12.KeyPress
        If e.KeyChar = Chr(13) Then 'Chr(13) is the Enter Key
            'Runs the Button1_Click Event
            btncalculate_Click(Me, EventArgs.Empty)
        End If
    End Sub

    'When hitting the enter button it calculates, only when focus is on a trick textbox
    Private Sub txtplayer1trick1_KeyPress(ByVal sender As System.Object, ByVal e As System.Windows.Forms.KeyPressEventArgs) Handles txtplayer1trick1.KeyPress
        If e.KeyChar = Chr(13) Then 'Chr(13) is the Enter Key
            'Runs the Button1_Click Event
            btncalculate_Click(Me, EventArgs.Empty)
        End If
    End Sub
    Private Sub txtplayer1trick2_KeyPress(ByVal sender As System.Object, ByVal e As System.Windows.Forms.KeyPressEventArgs) Handles txtplayer1trick2.KeyPress
